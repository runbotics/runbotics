        "moment": "^2.27.0",
        "nestjs-io-client": "0.1.0",
        "nodemailer": "^6.6.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "runbotics-actions-windows": "workspace:^1.0.0",
        "runbotics-common": "workspace:^1.0.0",
        "runbotics-sdk": "workspace:^1.0.0",
