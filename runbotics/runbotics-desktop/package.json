    "build": {
        "productName": "RunBotics",
        "electronVersion": "8.2.0",
        "appId": "com.runbotics.runbotics-desktop",
        "mac": {
            "category": "utility"
        },
        "directories": {
            "output": "release"
        },
        "files": [
            "dist/",
            "node_modules/",
            "package.json",
            "driver"
        ],
        "extraFiles": [
            ".env",
            "driver/"
        ],
        "linux": {
            "target": "deb"
        },
        "win": {
            "target": "nsis",
            "icon": "./public/icons/runbotics-512x512.png",
            "certificateFile": "./cert/selfsigncert.pfx",
            "certificatePassword": "hrG6WZ885VZgbr"
        },
        "nsis": {
            "allowToChangeInstallationDirectory": true,
            "oneClick": false,
            "runAfterFinish": false
        }
    },
    "devDependencies": {
        "@nestjs/cli": "^9.1.8",
        "@nestjs/schematics": "^9.0.4",
        "@nestjs/testing": "^9.2.1",
        "@types/asana": "^0.18.1",
        "@types/cron": "^2.0.0",
        "@types/jest": "^27.0.2",
        "@types/lodash": "^4.14.158",
        "@types/node": "^18.11.18",
        "@types/nodemailer": "^6.4.1",
        "@types/selenium-webdriver": "^4.0.12",
        "@types/supertest": "^2.0.11",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^5.48.0",
        "@typescript-eslint/parser": "^5.48.0",
        "@types/bpmn-moddle": "^5.1.6",
        "cross-env": "^7.0.3",
        "electron-builder": "^23.6.0",
        "eslint": "^8.31.0",
        "jest": "^27.2.5",
        "supertest": "^6.1.3",
        "ts-jest": "^27.0.3",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "files": [
        "dist",
        "driver",
        "start.sh",
        "cert",
        "public"
    ]
